<h1>This is a login page</h1>



<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:session, url: login_path) do |f| %>
      
      <%= f.label :email %>
      <%= f.email_field :email %>

      <%= f.label :password %>
      <%= f.password_field :password %>

      

      <%= f.submit "log in", class: "btn btn-primary" %>
      <%= link_to "Log out", logout_path, method: :delete %>
    <% end %>
  </div>
</div>
