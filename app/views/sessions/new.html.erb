<h1>Admin Login</h1>  
<%= form_for(:session, url: login_path) do %>  
  <label for="email">username</label>  
  <%= text_field_tag :email, params[:email] %>  
  <label for="password">Password</label>  
  <%= password_field_tag :password, params[:password] %>  
    
  <%= submit_tag "Login"%>  
<% end %>  
