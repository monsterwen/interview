
<%= will_paginate @users %>
<table style="width:500px">
  <thead>
      <tr>
          <th>User Name</th>
          <th style="text-align:right;">Email</th>
      </tr>
  </thead>
  <tbody>
      <% @users.each do |user| %>
          <tr>
            <td><%= user.name %></td>
            <td style="text-align:right;"><%= user.email %></td>
          </tr>
      <% end %>
  </tbody>
</table>
<%= link_to "back to all users", users_url %>
<%= will_paginate @users %>
